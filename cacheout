   bannerADtarget[2]="vnet";
   bannerAD[3]="images/img04.jpg";
   bannerADtarget[3]="vnet";
   bannerAD[4]="images/img05.jpg";
   bannerADtarget[4]="vnet";   
   bannerAD[5]="images/img06.jpg";
   bannerADtarget[5]="vnet";
   bannerAD[6]="images/img07.jpg";
   bannerADtarget[6]="vnet";
   bannerAD[7]="images/img08.jpg";
   bannerADtarget[7]="vnet";   
     
   
      
   var preloadedimages=new Array();
   for (i=0;i<bannerAD.length;i++){
      preloadedimages[i]=new Image();
      preloadedimages[i].src=bannerAD[i];
   }

function setTransition(){
   if (document.all){
      bannerADrotator.filters.revealTrans.Transition=23;
      bannerADrotator.filters.revealTrans.apply();
   }
}


function playTransition(){
   if (document.all)
      bannerADrotator.filters.revealTrans.play()
}

function nextAd(){
   if(adNum<bannerAD.length-1)adNum++ ;
      else adNum=0;
   setTransition();
   document.images.bannerADrotator.src=bannerAD[adNum];
   playTransition();
   theTimer=setTimeout("nextAd()", 6000);
}

function jump2url(){
   jumpUrl=bannerADlink[adNum];
   jumpTarget=bannerADtarget[adNum];
   
   if (jumpUrl != ''){
      if (jumpTarget != '')window.open(jumpUrl,jumpTarget);
      else window.open(jumpUrl,"","width=100,height=100,toolbar=no,location=no,directories=no,status=no,scrollbars=no,resizable=no");
   }
}
function displayStatusMsg() { 
   status=bannerADlink[adNum];
   document.returnValue = true;
}
function MM_openBrWindow(theURL,winName,features) { //v2.0
  window.open(theURL,winName,features);
}
