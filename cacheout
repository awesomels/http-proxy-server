   var bannerAD=new Array();
   var bannerADlink=new Array();
   var bannerADtarget=new Array();
   var adNum=0;
//图片的路径与链接的地址

//格林小镇实景图
   bannerAD[0]="images/img01.jpg";
   bannerADtarget[0]="vnet"; 
